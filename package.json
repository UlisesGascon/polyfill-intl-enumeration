{
  "name": "polyfill-intl-enumeration",
  "version": "0.0.9",
  "description": "Polyfill for intl enumeration",
  "main": "src/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "webpack --config webpack.config.js --mode production",
    "test": "cross-env NODE_ENV=testing jest --verbose",
    "test:coverage": "cross-env NODE_ENV=testing jest --verbose  --coverage",
    "test:watch": "cross-env NODE_ENV=testing jest --verbose --watchAll",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UlisesGascon/polyfill-intl-enumeration.git"
  },
  "keywords": [
    "polyfill",
    "intl",
    "enumeration"
  ],
  "standard": {
    "env": [
      "jest"
    ]
  },
  "author": "Ulises Gascon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UlisesGascon/polyfill-intl-enumeration/issues"
  },
  "homepage": "https://github.com/UlisesGascon/polyfill-intl-enumeration#readme",
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "cross-env": "7.0.3",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-config": "28.1.1",
    "standard": "17.0.0",
    "standard-version": "9.5.0",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  },
  "files": [
    "dist"
  ]
}
